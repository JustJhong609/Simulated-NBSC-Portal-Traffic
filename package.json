{
  "name": "nbsc-portal-traffic-dashboard",
  "version": "1.0.0",
  "description": "IP Geolocation and Reverse Geocoding Dashboard for Simulated NBSC Portal Traffic",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "build": "cd frontend && npm run build",
    "simulate": "cd data-simulator && python generate_traffic.py",
    "deploy": "cd frontend && npm run deploy"
  },
  "keywords": [
    "geolocation",
    "dashboard",
    "visualization",
    "nbsc"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
